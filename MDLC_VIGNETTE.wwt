     code : |1-
      // Résumé : Initialise le modèle de champs
      // Syntaxe :
      // InitVignette ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      Procédure InitVignette()
      
      	// Récupère l'alias du modèle de champs
      	__ALIAS = MoiMême..Alias
      	
      	// Récupère l'alias du champ image
      	__IMAGEALIAS = IMG_MDLIMAGE..Alias
      	
      	IMG_MDLIMAGE..HTMLAvant = ChaîneConstruit([
      	<div id='box%1' name='box%1' style='padding:4px; position:absolute; top: 0px; left: -9999px; visibility: hidden; color:#fff; background: white;'>
      		<img id='thb%1' name='thb%1' vspace=4 hspace=4 border=0 onclick='%4_CACHEIMAGE();' style='cursor: url(/%5/cursor/zoomout.cur), pointer'>
      	</div>
      	<div style='width: %2px; height: %3px'>
      	], __IMAGEALIAS, IMG_MDLIMAGE..Largeur, IMG_MDLIMAGE..Hauteur, __ALIAS, RépertoireWeb)
      	
      	IMG_MDLIMAGE..HTMLAprès = "</div>"
