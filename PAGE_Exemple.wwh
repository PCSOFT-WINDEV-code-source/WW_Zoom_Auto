              internal_properties : CAAAAAgAAABVl+WDGDQutARQ4Y7bWd610fa+crnB8rpka1SMvd7IGx1Ol9hw/aALhgzBkSShsf6fPUgSS/d21iSxdFCytECk8520/av5o6LFkJ4TqDWKSN+7r1f2eeG+FZ3VVWy1pSSoW2l6lPugcq/QRRahlkvpFn81YWCq2v+8habS8Bxu4NBQOG885Opvj5SMf7ImI5v1s5m+2LD8relcuyCNcdJBZn2qOw79ScKGzQAZ9gyM8Ad8H/fRKcQv47B5Ag==
              procedure_id : 1822033892731712861
              type_code : 14
              code : |1-
               // Résumé : Anime l'apparition de l'image en taille réelle
               // Syntaxe :
               // Nouvelle_Procédure ()
               //
               // Paramètres :
               //	Aucun
               // Valeur de retour :
               // 	Aucune
               //
               Procédure Animation()
               
               	pclBox 		est un objet dynamique
               	nOpacite 	est un entier
               
               	// Récupère la boîte de l'image en taille réelle
               	pclBox   = RecupereObjetPage("box" + __IMAGEALIAS)
               		
               	nOpacite = val(pclBox:getAttribute("opacite"))
               	nOpacite += 10	
               		
               	si nOpacite > 100 ALORS
               		
               		// Fin du timer
               		fintimer(gnTimerAnimation)
               		gnTimerAnimation = 0
               		retour
               	FIN
               	
               	SetOpacite(pclBox, nOpacite)	
              type : 458752
              role : N
            -
              name : CacheImage
              internal_properties : 
              procedure_id : 1822033892731778397
              type_code : 14
              code : |1-
               // Résumé : Cache l'image en taille réelle
               // Syntaxe :
               // Nouvelle_Procédure ()
               //
               // Paramètres :
               //	Aucun
               // Valeur de retour :
               // 	Aucune
               //
               Procédure CacheImage()
               
               	pclBox est un objet dynamique
               		
               	// Arrête l'animation si elle est en cours
               	si gnTimerAnimation <> 0 alors
               		fintimer(gnTimerAnimation)
               		gnTimerAnimation = 0
               	fin
               	
               	// Récupère la boîte
               	pclBox = RecupereObjetPage("box" + __IMAGEALIAS)
               	
               	SetOpacite(pclBox, 0)
               	pclBox:style:visibility = "hidden"
               	pclBox:style:left		= "-9999px"
               	pclBox:style:top		= "0px"
               	pclBox:style:zIndex 	= "0"
