              code : |1-
               // Résumé : <indiquez ici ce que fait la procédure>
               // Syntaxe :
               //[ <Résultat> = ] MaxZIndex ()
               //
               // Paramètres :
               //	Aucun
               // Valeur de retour :
               // 	entier sur 8 octets : // 	Aucune
               //
               // Exemple :
               // Indiquez ici un exemple d'utilisation.
               //
               Procédure MaxZIndex()
               
               	extern document
               
               	nIndex est un entier = 0
               	tabListe est un tableau de 0 objet dynamique
               
               	si document:all alors
               		
               		// Récupère tous les objets de la page
               		tabListe = document:all
               	sinon
               		
               		// Récupère tous les objets de la page
               		tabListe = document:getElementsByTagName("div")
               	fin
               	
               	pour i = 1 _a_ Dimension(tabListe)
               		// On récupère le zIndex si il est plus grand
               		pclDiv est un objet dynamique = tabListe[i]
               		si pclDiv <> null _et_ pclDiv:style alors
               			si nIndex < val(pclDiv:style:zIndex) alors				
               				nIndex = Val(pclDiv:style:zIndex)
               			FIN
               		fin
               	FIN
               	
               renvoyer (nIndex + 1)
