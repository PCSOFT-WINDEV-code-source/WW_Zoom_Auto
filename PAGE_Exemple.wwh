                     code : |1+
                      
                      
                      EXTERNE window
                      
                      pclBox		est un objet dynamique
                      pclThumb 	est un objet dynamique
                      pclChamp 	est un objet dynamique
                      nLargeur	est un entier
                      nHauteur	est un entier
                      rRatioL		est un réel = 1.0
                      rRatioH		est un réel = 1.0
                      rRatio 		est un réel = 1.0
                      nPosX		est un entier
                      nPosY		est un entier
                      
                      // Récupère l'image en taille réelle
                      pclThumb = RecupereObjetPage("thb" + __IMAGEALIAS)
                      pclBox   = RecupereObjetPage("box" + __IMAGEALIAS)
                      pclChamp = RecupereObjetPage(__IMAGEALIAS)
                      
                      // Si on n'a pas conservé la largeur d'origine
                      SI pclThumb:getAttribute("orgwidth") ALORS
                      	
                      	// Récupère la largeur d'origine conservée
                      	nLargeur = pclThumb:getAttribute("orgwidth") 		
                      SINON
                      	
                      	// Récupère la largeur d'origine
                      	nLargeur = pclThumb:offsetWidth
                      	
                      	// Sauve la largeur d'origine dans un attribut sur l'objet
                      	pclThumb:setAttribute("orgwidth", nLargeur)
                      FIN	
                      
                      // Si on n'a pas conservé la hauteur d'origine
                      SI pclThumb:getAttribute("orgheight") ALORS
                      	
                      	// Récupère la hauteur d'origine conservée
                      	nHauteur = pclThumb:getAttribute("orgheight") 	
                      SINON
                      	
                      	// Récupère la hauteur d'origine
                      	nHauteur = pclThumb:offsetHeight
                      	
                      	// Sauve la hauteur d'origine dans un attribut sur l'objet
                      	pclThumb:setAttribute("orgheight", nHauteur)
                      FIN
                      
                      // Ratio de l'image
                      SI nLargeur > nHauteur ALORS
                      	rRatio = nHauteur / nLargeur 
                      SINON 
                      	rRatio = nLargeur / nHauteur 
                      FIN
                      
                      // Ratio de réduction en largeur
                      SI nLargeur > __LARGMAX ALORS	
                      	rRatioL = __LARGMAX / nLargeur	
                      	nLargeur = __LARGMAX
                      	nHauteur = nHauteur * rRatioL
                      FIN
                      
                      // Ratio de réduction en hauteur
                      SI nHauteur > __HAUTMAX ALORS	
                      	rRatioH = __HAUTMAX / nHauteur	
                      	nHauteur = __HAUTMAX
                      	nLargeur = nLargeur * rRatioH
                      FIN
                      
                      // Calcule la position de l'image
                      nPosX = (MoiMême..Largeur - nLargeur) / 2 + MoiMême..X
                      nPosY = (MoiMême..Hauteur - nHauteur) / 2 + MoiMême..Y
                      
                      // Ramène la position à 0 si elle est inférieure
                      SI nPosX < 0 ALORS nPosX = 0
                      SI nPosY < 0 ALORS nPosY = 0
                      SI nPosX + nLargeur > window:document:body:offsetWidth ALORS nPosX = window:document:body:offsetWidth - nLargeur - 64
                      SI nPosY + nHauteur > window:document:body:offsetHeight ALORS nPosY = window:document:body:offsetHeight - nHauteur - 64
                      
                      pclBox:removeAttribute("opacite")
                      
                      // Redimensionne l'image
                      pclThumb:style:width 		= nLargeur
                      pclThumb:style:height 		= nHauteur
                      
                      // Affiche l'image
                      pclBox:style:left			= nPosX + "px"
                      pclBox:style:top			= nPosY + "px"
                      pclBox:style:zIndex 		= MaxZIndex()
                      pclBox:style:visibility 	= "visible"
                      pclThumb:style:visibility 	= "visible"
                      
                      // Lance l'animation
                      gnTimerAnimation = Timer(Animation, 2)
