                      	pclThumb:setAttribute("orgheight", nHauteur)
                      FIN
                      
                      // Ratio de l'image
                      SI nLargeur > nHauteur ALORS
                      	rRatio = nHauteur / nLargeur 
                      SINON 
                      	rRatio = nLargeur / nHauteur 
                      FIN
                      
                      // Ratio de réduction en largeur
                      SI nLargeur > __LARGMAX ALORS	
                      	rRatioL = __LARGMAX / nLargeur	
                      	nLargeur = __LARGMAX
                      	nHauteur = nHauteur * rRatioL
                      FIN
                      
                      // Ratio de réduction en hauteur
                      SI nHauteur > __HAUTMAX ALORS	
                      	rRatioH = __HAUTMAX / nHauteur	
                      	nHauteur = __HAUTMAX
                      	nLargeur = nLargeur * rRatioH
                      FIN
                      
                      // Calcule la position de l'image
                      nPosX = (MoiMême..Largeur - nLargeur) / 2 + MoiMême..X
                      nPosY = (MoiMême..Hauteur - nHauteur) / 2 + MoiMême..Y
                      
                      // Ramène la position à 0 si elle est inférieure
                      SI nPosX < 0 ALORS nPosX = 0
                      SI nPosY < 0 ALORS nPosY = 0
                      SI nPosX + nLargeur > window:document:body:offsetWidth ALORS nPosX = window:document:body:offsetWidth - nLargeur - 64
                      SI nPosY + nHauteur > window:document:body:offsetHeight ALORS nPosY = window:document:body:offsetHeight - nHauteur - 64
                      
                      pclBox:removeAttribute("opacite")
                      
                      // Redimensionne l'image
                      pclThumb:style:width 		= nLargeur
                      pclThumb:style:height 		= nHauteur
                      
                      // Affiche l'image
                      pclBox:style:left			= nPosX + "px"
                      pclBox:style:top			= nPosY + "px"
                      pclBox:style:zIndex 		= MaxZIndex()
                      pclBox:style:visibility 	= "visible"
                      pclThumb:style:visibility 	= "visible"
                      
                      // Lance l'animation
                      gnTimerAnimation = Timer(Animation, 2)
