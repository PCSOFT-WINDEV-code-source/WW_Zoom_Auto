            -
              name : Preload
              internal_properties : 
              procedure_id : 1822033892731647325
              type_code : 14
              code : |1-
               // Résumé : Pré-charge les image
               // Syntaxe :
               // Preload (<sAlias> est chaîne, <sImageMiniature> est chaîne, <sImageTailleReelle> est chaîne)
               //
               // Paramètres :
               //	sAlias (chaîne) : Alias du champ image
               //	sImageMiniature (chaîne) : Chemin de l'image miniature
               //	sImageTailleReelle (chaîne) : Chemin de l'image en taille réelle
               // Valeur de retour :
               // 	Aucune
               //
               Procédure Preload(local sAlias est une chaine, local sImageMiniature est une chaine, local sImageTailleReelle est une chaine)
               
               	pclChamp 	est un objet dynamique
               	pclThumb	est un objet dynamique
               	
               	// Récupère l'objet HTML correspondant
               	pclChamp = RecupereObjetPage(sAlias)
               	
               	si pclChamp <> null ALORS
               		
               		// Pas d'image en taille réelle ?
               		si sImageTailleReelle = "" ALORS
               			
               			// On affichera donc l'image miniature
               			sImageTailleReelle = sImageMiniature
               		FIN
               		
               		// On conserve le chemin de l'image dans la balise elle-même
               		pclChamp:setAttribute("thumb", (sImageTailleReelle))		
               		pclChamp:style:cursor = "url(" + repertoireweb() + "cursor/zoomin.cur), pointer"
               						
               		// Récupère l'image taille réelle
               		pclThumb = RecupereObjetPage("thb" + sAlias)
               		pclThumb:src = sImageTailleReelle
               	FIN
               	
               	CacheImage()
