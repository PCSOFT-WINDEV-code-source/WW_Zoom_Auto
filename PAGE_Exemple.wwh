               // Résumé : Anime l'apparition de l'image en taille réelle
               // Syntaxe :
               // Nouvelle_Procédure ()
               //
               // Paramètres :
               //	Aucun
               // Valeur de retour :
               // 	Aucune
               //
               Procédure Animation()
               
               	pclBox 		est un objet dynamique
               	nOpacite 	est un entier
               
               	// Récupère la boîte de l'image en taille réelle
               	pclBox   = RecupereObjetPage("box" + __IMAGEALIAS)
               		
               	nOpacite = val(pclBox:getAttribute("opacite"))
               	nOpacite += 10	
               		
               	si nOpacite > 100 ALORS
               		
               		// Fin du timer
               		fintimer(gnTimerAnimation)
               		gnTimerAnimation = 0
               		retour
               	FIN
               	
               	SetOpacite(pclBox, nOpacite)	
